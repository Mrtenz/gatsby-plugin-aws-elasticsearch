{
  "name": "@parataxic/gatsby-plugin-aws-elasticsearch",
  "version": "0.2.1",
  "description": "A plugin for Gatsby to synchronise content between Gatsby and AWS Elasticsearch",
  "author": "Maarten Zuidhoorn <maarten@zuidhoorn.com>",
  "homepage": "https://github.com/Mrtenz/gatsby-plugin-aws-elasticsearch",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mrtenz/gatsby-plugin-aws-elasticsearch.git"
  },
  "bugs": {
    "url": "https://github.com/Mrtenz/gatsby-plugin-aws-elasticsearch/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "aws",
    "elasticsearch"
  ],
  "license": "MIT",
  "main": "index.js",
  "sideEffects": false,
  "engines": {
    "node": ">=12"
  },
  "files": [
    "src",
    "typings",
    "index.js",
    "gatsby-node.js"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "yarn clean && yarn build:source",
    "build:source": "babel --extensions '.rs' src --out-dir lib",
    "lint": "yarn run lint:source && yarn run lint:format",
    "lint:source": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js,.jsx",
    "lint:format": "prettier --check --ignore-path .gitignore '**/*.{ts,tsx,js,jsx,json}'",
    "test": "jest",
    "format": "prettier --write --ignore-path .gitignore '**/*.{ts,tsx,js,jsx,json}'",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "aws4": "^1.10.1",
    "eslint": "^7.27.0",
    "node-fetch": "^2.6.1",
    "superstruct": "^0.15.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "babel-jest": "^26.5.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "gatsby": "^2.24.67",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
